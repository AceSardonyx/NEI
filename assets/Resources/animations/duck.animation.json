{
	"format_version" : "1.8.0",
   "animations": {
    "animation.duck.baby_transform": {
      "loop": true,
      "bones": {
        "head": {
          "scale": 2.0
        }
      }
    },
    "animation.duck.general": {
      "loop": true,
      "bones": {
        "wing0": {
          "rotation": [ 0.0, 0.0, 0.0 ]
        },
        "wing1": {
          "rotation": [ 0.0, 0.0, 0.0 ]
        }
      }
    },
    "animation.duck.move": {
      "anim_time_update": "query.modified_distance_moved",
      "loop": true,
      "bones": {
        "leg0": {
          "rotation": [ "math.cos(query.anim_time * 38.17) *  80.0", 0.0, 0.0 ]
        },
        "leg1": {
          "rotation": [ "math.cos(query.anim_time * 38.17) * -80.0", 0.0, 0.0 ]
        }
      }
    }
  }
}
