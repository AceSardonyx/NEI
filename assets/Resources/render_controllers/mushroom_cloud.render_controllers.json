{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.cap": {
      "geometry": "Geometry.cap",
      "materials": [ { "*": "Material.toratoratora" } ],
      "textures": [ "Texture.default" ],
      "overlay_color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": 1.0
      },
      "uv_anim": {
        "offset": [
          "(Math.floor(query.life_time * 20.0) + query.frame_alpha) * 0.001",
          "-(Math.floor(query.life_time * 20.0) + query.frame_alpha) * 0.001"
        ],
        "scale": [ 1.0, 1.0 ]
      },
      "light_color_multiplier": 0.5,
      "ignore_lighting": true
    },
    "controller.render.mushroom_clouda": {
      "geometry": "Geometry.toratoratora",
      "materials": [ { "*": "Material.toratoratora" } ],
      "textures": [ "Texture.default" ],
      "overlay_color": {
        "r": 1.0,
        "g": 1.0,
        "b": 1.0,
        "a": 1.0
      },
      "uv_anim": {
        "offset": [
          "0",
          "(Math.floor(query.life_time * 20.0) + query.frame_alpha) * 0.001"
        ],
        "scale": [ 1.0, 1.0 ]
      },
      "light_color_multiplier": 0.5,
      "ignore_lighting": true
    },
    "controller.render.mushroom_cloud": {
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ]
    },
    "controller.render.nuke": {
      "geometry": "Geometry.default",
      "materials": [ { "*": "Material.default" } ],
      "textures": [ "Texture.default" ]
    }
  }
}
